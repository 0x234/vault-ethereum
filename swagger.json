    "/addresses/{address}": {
      "post": {
        "tags": [
          "Addresses"
        ],
        "summary": "Handler returning Account Names for an Address.",
        "operationId": "pathAddressesRead",
        "parameters": [
          {
            "type": "string",
            "x-go-name": "Address",
            "description": "The address to lookup",
            "name": "address",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "accountNames",
            "schema": {
              "$ref": "#/definitions/accountNames"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "accountNames": {
      "description": "AccountNames stores the names of the account to allow reverse lookup by address",
      "type": "object",
      "properties": {
        "names": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-go-name": "Names"
        }
      },
      "x-go-name": "AccountNames",
      "x-go-package": "github.com/zambien/vault-ethereum"
    }
  },
